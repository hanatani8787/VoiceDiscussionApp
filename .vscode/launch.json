{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug iOS",
      "program": "${workspaceFolder}/.vscode/launchReactNative.js",
      "request": "launch",
      "type": "reactnative",
      "cwd": "${workspaceFolder}",
      "platform": "ios",
      "sourceMaps": true,
      "outDir": "${workspaceFolder}/.vscode/.react",
      "trace": "verbose",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/react-native",
      "port": 8081
    }
  ]
}
